{
    "name": "alish-io/file-manager",
    "description": "File manager for Alish",
    "keywords": [
        "alish",
        "file",
        "manager"
    ],
    "authors": [
        {
            "name": "Mahboub Hosseinzadeh",
            "email": "git@alish.co.ir",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/webalish/file-manager",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "ext-zip": "*",
        "ext-json": "*",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "league/flysystem": "^1.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Alish\\FileManager\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alish\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}
